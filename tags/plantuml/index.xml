&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl"> if (分支条件2判断) then (no)
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl"> :条件2不成立时执行的动作;
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl"> else
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl"> if (条件3判断) then (yes)
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl"> :条件3成立时的动作;
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl"> else (no)
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl"> :条件3不成立时的动作;
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl"> endif
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl"> endif
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl"> :顺序步骤3处理;
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">endif
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">if (条件4判断) then (yes)
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">:条件4成立的动作;
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">else
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl"> if (条件5判断) then (yes)
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl"> :条件5成立时的动作;
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl"> else (no)
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl"> :条件5不成立时的动作;
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl"> endif
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">endif
