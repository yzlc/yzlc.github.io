&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">start
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">:步骤1处理;
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">:步骤2处理;
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">if (条件1判断) then (true)
