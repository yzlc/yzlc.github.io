&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">reg delete HKEY_LOCAL_MACHINE&lt;span class="se">\S&lt;/span>YSTEM&lt;span class="se">\C&lt;/span>ontrolSet002&lt;span class="se">\S&lt;/span>ervices&lt;span class="se">\E&lt;/span>ventlog&lt;span class="se">\A&lt;/span>pplication&lt;span class="se">\M&lt;/span>YSQL /f
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">reg delete HKEY_LOCAL_MACHINE&lt;span class="se">\S&lt;/span>YSTEM&lt;span class="se">\C&lt;/span>urrentControlSet&lt;span class="se">\S&lt;/span>ervices&lt;span class="se">\E&lt;/span>ventlog&lt;span class="se">\A&lt;/span>pplication&lt;span class="se">\M&lt;/span>YSQL /f
